msgstr "Forma: (w, h, 3)"

#: ../PygameTools.rst:936
msgid "Data type: uint8"
msgstr "Tipo de datos: uint8"

#: ../PygameTools.rst:940
msgid "numpy.ndarray[np.uint8_t, ndim=3]"
msgstr "numpy.ndarray[np.uint8_t, ndim=3]"

